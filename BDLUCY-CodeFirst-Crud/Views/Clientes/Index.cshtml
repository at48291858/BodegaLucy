@model IEnumerable<BDLUCY_CodeFirst_Crud.Models.Cliente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Nombre_Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido_Paterno_Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Apellido_Materno_Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Tipo_Documento_Cliente)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Documento_Cliente)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_Cliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido_Paterno_Cliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Apellido_Materno_Cliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tipo_Documento_Cliente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Documento_Cliente)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Codigo_Cliente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Codigo_Cliente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Codigo_Cliente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
